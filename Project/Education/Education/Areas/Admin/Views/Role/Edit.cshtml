@model Education.Areas.Admin.Model.AspNetRole

@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="content-header">
    <table cellspacing="0">
        <tbody>
            <tr>
                <td style="width:50%;"><h3 class="icon-head head-cms-page">Xem thông tin quyền</h3></td>
                <td class="form-buttons">
                    <button class="scalable Back" name="submit" onclick="setLocation('/Admin/Role/Index')" type="button" value="Back"><span>Quay lại</span></button>
                </td>
            </tr>
        </tbody>
    </table>
</div>

<div class="entry-edit">
    <div class="entry-edit-head">
        <h4 class="icon-head head-edit-form fieldset-legend"><a href="#">Xem thông tin quyền</a></h4>
        <div class="form-buttons"></div>
    </div>
    <div class="fieldset " id="user_base_fieldset">
        <div class="hor-scroll">
            <table cellspacing="0" class="form-list">
                <tbody>
                    <tr>
                        <td class="label">
                            @Html.LabelFor(model => model.Name)
                        </td>
                        <td class="value">
                            @*@Html.TextBoxFor(model => model.Name, new { @class = "input-text" })
                            <br />@Html.ValidationMessageFor(model => model.Name)*@
                            @Model.Name
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>